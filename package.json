{
  "name": "api-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/infra/web/server.js",
    "dev": "set NODE_ENV=dev&& nodemon -r tsconfig-paths/register src/infra/web/server.ts",
    "prod": "nodemon -r tsconfig-paths/register src/infra/web/server.ts",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/infra/web/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist  --copy-files",
    "tsc": "tsc",
    "test": "npm run tsc -- --noEmit && jest",
    "swagger-autogen": "node swagger.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@swc/jest": "^0.2.20",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^27.5.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.172",
    "@types/swagger-ui-express": "^4.1.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "reflect-metadata": "^0.1.13",
    "swagger-autogen": "^2.21.1",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.4",
    "tsyringe": "^4.6.0"
  }
}
